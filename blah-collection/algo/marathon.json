{
  "id": "collection-streaming",
  "cpus": 0.5,
  "mem": 1024.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "blah/collection-algo",
      "network": "BRIDGE"
    }
  },
  "args": ["streaming"],
  "env": {
    "KAFKA_URL": "$BLAH_HOST:9092",
    "ZOOKEEPER_URL": "$BLAH_HOST:2181",
    "ELASTICSEARCH_URL": "http://$BLAH_HOST:9200",
    "SPARK_MASTER": "mesos://zk://$BLAH_HOST:2181/mesos",
    "STREAMING_BATCH_INTERVAL": "5"
  }
}
