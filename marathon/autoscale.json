{
  "id": "autoscale",
  "cpus": 0.1,
  "mem": 128.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "blah/autoscale",
      "network": "BRIDGE",
      "portMappings": [
        {"containerPort": 8003, "hostPort": 0, "servicePort": 8003, "protocol": "tcp"}
      ]
    }
  },
  "env": {
    "RUST_LOG": "info",
    "RUST_BACKTRACE": "1"
  },
  "args": [
    "--host", "$BLAH_HOST", "--sse"
  ],
  "labels":{
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_VHOST": "autoscale.blah.local",
    "HAPROXY_0_BACKEND_HTTP_HEALTHCHECK_OPTIONS": "  option  httpchk GET {healthCheckPath} HTTP/1.1\\r\\nHost:\\ autoscale.blah.local\n  timeout check {healthCheckTimeoutSeconds}s\n"
  },
  "healthChecks": [{
    "protocol": "HTTP",
    "path": "/healthcheck",
    "intervalSeconds": 10
  }]
}
