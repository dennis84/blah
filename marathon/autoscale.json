{
  "id": "autoscale",
  "cpus": 0.1,
  "mem": 128.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "blah/autoscale",
      "network": "BRIDGE"
    }
  },
  "env": {
    "RUST_LOG": "info",
    "RUST_BACKTRACE": "1"
  },
  "args": [
    "--host", "$BLAH_HOST"
  ]
}
