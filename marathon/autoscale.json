{
  "id": "autoscale",
  "cpus": 0.1,
  "mem": 128.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "blah/autoscale",
      "network": "BRIDGE",
      "portMappings": [
        {"containerPort": 8003, "hostPort": 0, "servicePort": 8003, "protocol": "tcp"}
      ]
    }
  },
  "env": {
    "RUST_LOG": "info",
    "RUST_BACKTRACE": "1"
  },
  "args": [
    "--host", "$BLAH_HOST", "--sse"
  ]
}
