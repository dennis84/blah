{
  "id": "count-streaming",
  "cpus": 0.5,
  "mem": 512.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "blah/algo:0.1.0",
      "network": "BRIDGE"
    }
  },
  "args": ["count-streaming"],
  "env": {
    "KAFKA_URL": "kafka.marathon.mesos:9092",
