{
  "id": "elasticsearch",
  "cpus": 0.1,
  "mem": 512.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "blah/elasticsearch",
      "network": "BRIDGE",
      "portMappings": [
        {"containerPort": 9200, "hostPort": 0, "servicePort": 9200, "protocol": "tcp"},
        {"containerPort": 9300, "hostPort": 0, "servicePort": 9300, "protocol": "tcp"}
      ]
    }
  }
}
