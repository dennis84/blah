{
  "id": "logstash",
  "cpus": 0.1,
  "mem": 512.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "blah/logstash",
      "network": "BRIDGE",
      "portMappings": [
        {"containerPort": 5044, "hostPort": 0, "servicePort": 5044, "protocol": "tcp"}
      ]
    }
  },
  "args": ["--quiet"],
  "env": {
    "ELASTICSEARCH_URL": "$BLAH_HOST:9200"
  },
  "labels":{
    "HAPROXY_GROUP": "external"
  }
}
