{
  "id": "serving",
  "cpus": 0.1,
  "mem": 512.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "blah/serving:0.1.0",
      "network": "BRIDGE",
      "portMappings": [
        {"containerPort": 8001, "hostPort": 0, "servicePort": 8001, "protocol": "tcp"}
      ]
    }
  },
  "env": {
    "ZOOKEEPER_URL": "$BLAH_HOST:2181",
    "KAFKA_URL": "$BLAH_HOST:31000",
    "ELASTICSEARCH_URL": "$BLAH_HOST:9200",
    "CHRONOS_URL": "$BLAH_HOST:8081"
  },
  "labels":{
    "AUTOSCALE_MAX_INSTANCES": "10",
    "AUTOSCALE_MEM_PERCENT": "80",
    "AUTOSCALE_CPU_PERCENT": "80",
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_VHOST": "serving.blah.local",
    "HAPROXY_0_BACKEND_HTTP_HEALTHCHECK_OPTIONS": "  option  httpchk GET {healthCheckPath} HTTP/1.1\\r\\nHost:\\ serving.blah.local\n  timeout check {healthCheckTimeoutSeconds}s\n"
  },
  "healthChecks": [{
    "protocol": "HTTP",
    "path": "/",
    "intervalSeconds": 10
  }]
}
