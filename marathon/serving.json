{
  "id": "serving",
  "cpus": 1,
  "mem": 1024.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "blah/serving:0.1.0",
      "network": "BRIDGE",
      "portMappings": [
        {"containerPort": 8001, "hostPort": 0, "servicePort": 8001, "protocol": "tcp"}
      ]
    }
  },
  "env": {
    "ZOOKEEPER_URL": "$BLAH_HOST:2181",
    "KAFKA_URL": "$BLAH_HOST:31000",
    "ELASTICSEARCH_URL": "$BLAH_HOST:9200"
  },
  "labels":{
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_VHOST": "serving.blah.local"
  }
}
