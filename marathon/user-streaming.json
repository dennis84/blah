{
  "id": "user-streaming",
  "cpus": 0.5,
  "mem": 1024.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "blah/user-algo:0.1.0",
      "network": "BRIDGE"
    }
  },
  "args": ["streaming"],
  "env": {
    "KAFKA_URL": "$BLAH_HOST:31000",
    "ZOOKEEPER_URL": "$BLAH_HOST:2181",
    "HADOOP_URL": "hdfs://$BLAH_HOST:8020",
    "ELASTICSEARCH_URL": "http://$BLAH_HOST:9200",
    "SPARK_MASTER": "mesos://zk://$BLAH_HOST:2181/mesos",
    "STREAMING_BATCH_INTERVAL": "1"
  }
}
