{
  "id": "user-streaming",
  "cpus": 0.1,
  "mem": 128.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "blah/algo:0.1.0",
      "network": "BRIDGE"
    }
  },
  "args": ["user-streaming"],
  "env": {
    "KAFKA_URL": "kafka.marathon.mesos:9092",
    "ZOOKEEPER_URL": "zookeeper.marathon.mesos:2181",
    "HADOOP_URL": "hdfs://namenode.marathon.mesos:8020",
    "ELASTICSEARCH_URL": "elasticsearch.marathon.mesos:9200",
    "SPARK_MASTER": "mesos://zk://192.168.99.100:2181/mesos"
  }
}
